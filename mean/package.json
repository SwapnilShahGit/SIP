{
  "name": "rename",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "docker-rmi": "docker rmi gcr.io/lab-0/rename:v20160408",
    "docker-build": "npm run docker-rmi && docker build -t gcr.io/lab-0/rename:v20160411 .",
    "docker": "npm run docker-rmi && npm run docker-build && docker run -it --rm -p 8080:8080 -p 3001:3001 gcr.io/lab-0/rename:v20160411",
    "postinstall": "typings install"
  },
