    "fullcalendar": "^2.7.0-beta",
    "jquery": "^2.2.3",
    "jquery-ui": "^1.10.5",
    "primeng": "1.0.0-beta.1",
    "primeui": "4.1.7",
    "reflect-metadata": "0.1.2",
    "require": "^2.4.20",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.25",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
